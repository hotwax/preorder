A,WAAA,KAAW,IAAX,GAAuB,IAAA,CAAA,WAAA,CAAY,SAAnC,GAA4C,E,GAAA;AAAA;mBAAQ,CAAvD;AAAA,iB,CACU;;;;AAAA,eAFZ,CAEY,EACZ,YAAA,CAOC,qBAPD,EAOC;AANC,gBAAA,KAAiD,EAAjD;AAAA,iCAAA,WAAA;AAAA,iCAAA;AAAA,iBAMD;AALE,gBAAA,OAAK,EAAE,IAAA,CAAA,MAKT;AAJC,gBAAA,IAAI,EAAC,KAIN;AAHC,gBAAA,IAAI,EAAC,SAGN;AAFC,gBAAA,KAAK,EAAC;AAEP,eAPD,E;kCAMG;AAAA,yBAAkB,C,kCAAf,IAAA,CAAA,EAAA,CAAE,QAAF,C,GAAE;AAAA;mBAAa,CAAlB;AAAA,iB;;;;eANH,E;;AAAA,gB,WAAA,CADY,CAFZ;AAAA,a,CADsC;;;;AAAA,WAAxC,CAlBW,CAfX;AAAA,S,CAD4B;;;;AAAA,OAA9B,CAFa,CATb;AAAA,K,CA2DS;;;;AAAA,GA5DX,C","sourcesContent":["<template>\n  <ion-page>\n    <ion-header :translucent=\"true\">\n      <ion-toolbar>\n        <ion-buttons slot=\"start\">\n          <ion-menu-button></ion-menu-button>\n        </ion-buttons>\n        <ion-title style=\"font-family: Montserrat; font-weight: bold\">{{\n          $t(\"Settings\")\n        }}</ion-title>\n      </ion-toolbar>\n    </ion-header>\n\n    <ion-content :fullscreen=\"true\">\n      <ion-item style=\"font-family: Open Sans; font-weight: bold\">\n        <ion-label>{{ $t(\"Brand\") }}</ion-label>\n        <ion-select\n          @ionChange=\"updateBrand($event)\"\n          interface=\"popover\"\n          :value=\"selectedBrand\"\n        >\n          <ion-select-option value=\"\">{{ $t(\"All\") }}</ion-select-option>\n          <ion-select-option\n            v-bind:key=\"brand.id\"\n            v-for=\"brand in brands\"\n            :value=\"brand.id\"\n            >{{ brand.name }}</ion-select-option\n          >\n        </ion-select>\n      </ion-item>\n      <ion-item style=\"font-family: Open Sans\">\n        <ion-label>\n          {{\n            userProfile && userProfile.userTimeZone\n              ? userProfile.userTimeZone\n              : \"-\"\n          }}\n        </ion-label>\n        <ion-button\n          style=\"font-family: Open Sans; font-weight: bold\"\n          @click=\"changeTimeZone()\"\n          slot=\"end\"\n          fill=\"outline\"\n          color=\"dark\"\n          >{{ $t(\"Change\") }}</ion-button\n        >\n      </ion-item>\n      <ion-item style=\"font-family: Open Sans\">\n        <ion-label>\n          {{ userProfile !== null ? userProfile.partyName : \"\" }}\n        </ion-label>\n        <ion-button\n          style=\"font-family: Open Sans; font-weight: bold\"\n          @click=\"logout\"\n          slot=\"end\"\n          fill=\"outline\"\n          color=\"dark\"\n          >{{ $t(\"Logout\") }}</ion-button\n        >\n      </ion-item>\n    </ion-content>\n  </ion-page>\n</template>\n\n<script lang=\"ts\">\nimport { useStore } from \"@/store\";\nimport {\n  IonButton,\n  IonButtons,\n  IonContent,\n  IonHeader,\n  IonItem,\n  IonLabel,\n  IonMenuButton,\n  IonPage,\n  IonSelect,\n  IonSelectOption,\n  IonTitle,\n  IonToolbar,\n  modalController,\n} from \"@ionic/vue\";\nimport { defineComponent } from \"vue\";\nimport { mapGetters } from \"vuex\";\nimport TimeZoneModal from \"@/views/timezone-modal.vue\";\n\nexport default defineComponent({\n  name: \"settings\",\n  components: {\n    IonButton,\n    IonButtons,\n    IonContent,\n    IonHeader,\n    IonItem,\n    IonLabel,\n    IonMenuButton,\n    IonPage,\n    IonSelect,\n    IonSelectOption,\n    IonTitle,\n    IonToolbar,\n  },\n  setup() {\n    const store = useStore();\n    return { store };\n  },\n  data() {\n    return {\n      brands: JSON.parse(process.env.VUE_APP_BRANDS),\n    };\n  },\n  computed: {\n    ...mapGetters({\n      userProfile: \"user/getUserProfile\",\n      selectedBrand: \"user/getSelectedBrand\",\n    }),\n  },\n  methods: {\n    logout: function () {\n      this.store.dispatch(\"user/logout\").then(() => {\n        this.$router.push(\"/login\");\n      });\n    },\n    async changeTimeZone() {\n      const timeZoneModal = await modalController.create({\n        component: TimeZoneModal,\n      });\n      return timeZoneModal.present();\n    },\n    updateBrand(event: any) {\n      this.store.dispatch(\"user/setSelectedBrand\", {\n        selectedBrand: event.detail.value,\n      });\n    },\n  },\n});\n</script>\n"],"sourceRoot":""}]}